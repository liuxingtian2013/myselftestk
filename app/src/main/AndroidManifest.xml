<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shanxixinhe.device.service">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.shanxixinhe.application.AidlApplication"
        android:theme="@style/AppTheme">
        <activity android:name="com.shanxixinhe.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.shanxixinhe.service.DeviceService">
            <intent-filter>
                <action android:name="com.shanxixinhe.DeviceService" />
            </intent-filter>
        </service>
    </application>

</manifest>